{
  "name": "yajra/laravel-datatables-html",
  "description": "Laravel DataTables HTML builder plugin for Laravel 5.4+.",
  "keywords": [
    "laravel",
    "dataTables",
    "jquery",
    "html",
    "js"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Arjay Angeles",
      "email": "aqangeles@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/yajra/html"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "yajra/laravel-datatables-oracle": "7.*",
    "laravelcollective/html": "5.4.*"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Yajra\\Datatables\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
